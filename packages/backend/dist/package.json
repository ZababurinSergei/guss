{
  "name": "the-last-of-guss-backend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "node scripts/build.js",
    "start": "node dist/index.js",
    "db:create": "node scripts/create-database.js",
    "db:drop": "node scripts/drop-database.js",
    "migrate": "node scripts/migrate.js up",
    "migrate:up": "node scripts/migrate.js up",
    "migrate:down": "node scripts/migrate.js down",
    "migrate:reset": "node scripts/migrate.js reset",
    "migrate:status": "node scripts/migrate.js status",
    "db:setup": "npm run db:create && npm run migrate:up",
    "db:reset": "npm run migrate:reset && npm run migrate:up",
    "db:clean": "npm run db:cleanup && npm run migrate:up",
    "db:clear": "node scripts/clear-database.js",
    "db:cleanup": "node scripts/cleanup-database.js",
    "db:fresh": "npm run db:reset && npm run migrate:up",
    "db:recreate": "npm run db:reset && npm run db:create && npm run migrate:up"
  },
  "dependencies": {
    "the-last-of-guss-database": "*",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.1.0",
    "@fastify/static": "^8.3.0",
    "@libp2p/logger": "^6.2.2",
    "fastify": "^5.6.2",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "pino-pretty": "^13.1.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^3.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "sequelize-cli": "^6.6.3",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}